include $(GNUSTEP_MAKEFILES)/common.make

PACKAGE_NAME=TimeSliceAggregation
BUNDLE_NAME=TimeSliceAggregation
BUNDLE_EXTENSION=.bundle
BUNDLE_INSTALL_DIR=$(GNUSTEP_BUNDLES)/Triva
TimeSliceAggregation_PRINCIPAL_CLASS=TimeSliceAggregation

#
# Additional libraries
#

ifeq ($(FOUNDATION_LIB), apple)
  LDFLAGS += -F../Triva -framework Triva -framework General
else
  TimeSliceAggregation_BUNDLE_LIBS += -lTriva -lGeneral
  ADDITIONAL_LIB_DIRS += -L../Triva/Triva.framework/Versions/Current
endif

#
# Class files
#

TimeSliceAggregation_OBJC_FILES = \
  TimeSliceAggregation.m \
  TimeSliceAggregation+State.m \
  TimeSliceAggregation+Variable.m \
  TimeSliceAggregation+Link.m \
  TimeSliceAggregation+Debugging.m
TimeSliceAggregation_OBJCC_FILES=

TimeSliceAggregation_INCLUDE_DIRS += -I../ -I./

include $(GNUSTEP_MAKEFILES)/bundle.make

after-distclean::
after-all::
after-clean:: 
