include $(GNUSTEP_MAKEFILES)/common.make

PACKAGE_NAME=SpatialIntegration
BUNDLE_NAME=SpatialIntegration
BUNDLE_EXTENSION=.bundle
BUNDLE_INSTALL_DIR=$(GNUSTEP_BUNDLES)/Triva
SpatialIntegration_PRINCIPAL_CLASS=SpatialIntegration

ifeq ($(FOUNDATION_LIB), apple)
  LDFLAGS += -F../Triva -framework Triva -framework General
else
  ADDITIONAL_LIB_DIRS += -L../Triva/Triva.framework/Versions/Current
endif

SpatialIntegration_OBJC_FILES = SpatialIntegration.m
SpatialIntegration_OBJCC_FILES=

SpatialIntegration_INCLUDE_DIRS += -I../ -I./ @CPPFLAGS@
SpatialIntegration_LDFLAGS += @LDFLAGS@

include $(GNUSTEP_MAKEFILES)/bundle.make

