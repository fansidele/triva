GraphView_PRINCIPAL_CLASS = GraphView
BUNDLE_EXTENSION = .bundle
BUNDLE_INSTALL_DIR = $(GNUSTEP_BUNDLES)/@PROJECT_NAME@


#
# Resource files
#
GraphView_RESOURCE_FILES = \
Resources/Graph.gorm \


#
