include $(GNUSTEP_MAKEFILES)/common.make

PACKAGE_NAME=TimeIntegration
BUNDLE_NAME=TimeIntegration
BUNDLE_EXTENSION=.bundle
BUNDLE_INSTALL_DIR=$(GNUSTEP_BUNDLES)/Triva
TimeIntegration_PRINCIPAL_CLASS=TimeIntegration

ifeq ($(FOUNDATION_LIB), apple)
  LDFLAGS += -F../Triva -framework Triva -framework General
else
  ADDITIONAL_LIB_DIRS += -L../Triva/Triva.framework/Versions/Current
endif

TimeIntegration_OBJC_FILES = TimeIntegration.m 
TimeIntegration_OBJCC_FILES=

TimeIntegration_INCLUDE_DIRS += @CPPFLAGS@
TimeIntegration_LDFLAGS += @LDFLAGS@

include $(GNUSTEP_MAKEFILES)/bundle.make

after-distclean::
after-all::
after-clean:: 
